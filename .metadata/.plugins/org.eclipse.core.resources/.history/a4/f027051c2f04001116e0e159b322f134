spring:
	  jpa:
	    open-in-view: false

  datasource:
    url: jdbc:postgresql://localhost:5432/approval_db
    username: postgres
    password: admin
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        format_sql: true
    database-platform: org.hibernate.dialect.PostgreSQLDialect
